{
  "name": "nestor",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "chart.js": "^2.8.0",
    "concurrently": "^4.1.2",
    "cors": "^2.8.5",
    "electron-dl": "^1.14.0",
    "electron-is-dev": "^1.1.0",
    "express": "^4.17.1",
    "fuzzball": "^1.3.0",
    "is-electron": "^2.2.0",
    "jquery": "^3.4.1",
    "multer": "^1.4.2",
    "nodemon": "^1.19.4",
    "react": "^16.10.2",
    "react-bootstrap": "^1.0.0-beta.14",
    "react-chartjs-2": "^2.8.0",
    "react-csv": "^1.1.1",
    "react-dom": "^16.10.2",
    "react-file-reader": "^1.1.4",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.0.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "wait-on": "^3.3.0",
    "zerorpc": "git+https://github.com/fyears/zerorpc-node.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \" SET BROWSER=none&&npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "react-scripts build --em.main=build/electron.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^5.0.11",
    "electron-builder": "^20.44.4",
    "electron-packager": "^14.0.6",
    "electron-rebuild": "^1.8.6",
    "typescript": "^3.6.4"
  },
  "build": {
    "appId": "com.nist.nestor",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}

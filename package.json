{
  "name": "nestor",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "chart.js": "^2.9.3",
    "concurrently": "^4.1.2",
    "cors": "^2.8.5",
    "electron-dl": "^1.14.0",
    "electron-is-dev": "^1.1.0",
    "express": "^4.17.1",
    "fuzzball": "^1.3.0",
    "is-electron": "^2.2.0",
    "jquery": "^3.4.1",
    "multer": "^1.4.2",
    "nodemon": "^1.19.4",
    "papaparse": "^5.1.1",
    "pouchdb": "^7.1.1",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-chartjs-2": "^2.9.0",
    "react-csv": "^1.1.2",
    "react-dom": "^16.12.0",
    "react-file-reader": "^1.1.4",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.0.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "wait-on": "^3.3.0",
    "zerorpc": "^0.9.8"
  },
  "scripts": {
    "init": "npm install && ./node_modules/.bin/electron-rebuild",
    "init-windows": "npm install && ./node_modules/.bin/electron-rebuild.cmd",
    "start": "react-scripts start",
    "build-react": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "build-electron-windows": "./node_modules/.bin/electron-packager . --overwrite  --platform=win32 --arch=x64 --icon=build/icon.ico --asar --prune=true --out=release-builds",
    "build-electron-macos": "./node_modules/.bin/electron-packager . --overwrite   --platform=darwin --arch=x64 --icon=build/icon.icns --prune=true --out=release-builds",
    "build-electron-linux": "./node_modules/.bin/electron-packager . --overwrite   --platform=linux --arch=x64 --icon=build/icon.icns --asar --prune=true --out=release-builds",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "build-python": "pyinstaller api.spec --additional-hooks-dir=. --distpath pythondist --noconfirm",
    "build": "npm run build-react && npm run build-python && npm run build-electron-windows"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "electron": "^5.0.11",
    "electron-builder": "^22.2.5",
    "electron-packager": "^14.0.6",
    "electron-rebuild": "^1.10.0",
    "typescript": "^3.7.5"
  },
  "build": {
    "appId": "com.nist.nestor",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "icon": "build/icon.icns"
    },
    "win": {
      "icon": "build/icon.ico"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}